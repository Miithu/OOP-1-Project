public class Cricketer {
    private String name;
    private int age;
    private String gender;
    private String country;
    private String team;
    private String role;

    public Cricketer() {
        // Default constructor
    }

    public Cricketer(String name, int age, String gender, String country, String team, String role) {
        this.name = name;
        this.age = age;
        this.gender = gender;
        this.country = country;
        this.team = team;
        this.role = role;
    }

    // Getters and setters for each field

    public String getName() {
        return name;
    }

    public void setName(String name) {
        this.name = name;
    }

    public int getAge() {
        return age;
    }

    public void setAge(int age) {
        this.age = age;
    }

    public String getGender() {
        return gender;
    }

    public void setGender(String gender) {
        this.gender = gender;
    }

    public String getCountry() {
        return country;
    }

    public void setCountry(String country) {
        this.country = country;
    }

    public String getTeam() {
        return team;
    }

    public void setTeam(String team) {
        this.team = team;
    }

    public String getRole() {
        return role;
    }

    public void setRole(String role) {
        this.role = role;
    }

    @Override
    public String toString() {
        return "Cricketer {" +
                "Name='" + name + '\'' +
                ", Age=" + age +
                ", Gender='" + gender + '\'' +
                ", Country='" + country + '\'' +
                ", Team='" + team + '\'' +
                ", Role='" + role + '\'' +
                '}';
    }
}


